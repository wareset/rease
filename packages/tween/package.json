{
  "name": "@rease/tween",
  "version": "0.4.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "wip: @rease/tween",
  "keywords": [
    "rease",
    "tween"
  ],
  "license": "MIT",
  "author": "rease <cftcview@gmail.com>",
  "main": "index",
  "module": "index.mjs",
  "types": "types/index.d.ts",
  "files": [
    "README.md",
    "easing/**/*",
    "index.d.ts",
    "index.js",
    "index.mjs",
    "raf/**/*",
    "types/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./easing": {
      "import": "./easing/index.mjs",
      "require": "./easing/index.js",
      "types": "./types/easing.d.ts"
    },
    "./raf": {
      "import": "./raf/index.mjs",
      "require": "./raf/index.js",
      "types": "./types/raf.d.ts"
    }
  },
  "scripts": {
    "build": "dester -m",
    "dev": "dester -mw"
  },
  "devDependencies": {
    "dester": "1.0.x",
    "typescript": "5.7.2"
  }
}